programa {
  funcao inicio() {
    real N1, N2, N3, Total, X

    escreva ("Digite sua primeira nota: ")
    leia (N1)
    escreva ("Digite sua segunda nota: ")
    leia (N2)
    escreva ("Digite sua terceira nota: ")
    leia (N3)

    Total=N1+N2+N3
    X=Total/3

    escreva ("Media das notas: ", X)
  }
}
